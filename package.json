{
  "name": "aisdk-codex",
  "version": "0.1.0",
  "description": "AI SDK provider for the Codex API (ChatGPT Pro/Plus)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "bun run clean && tsc",
    "prepack": "bun run build",
    "prepublishOnly": "bun run build",
    "pack:check": "bun pm pack --dry-run"
  },
  "dependencies": {
    "@ai-sdk/provider": "^3.0.8",
    "@ai-sdk/provider-utils": "^4.0.14",
    "eventsource-parser": "^3.0.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "ai": "^6.0.77",
    "typescript": "^5.9.0"
  },
  "peerDependencies": {
    "ai": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1jmdev/aisdk-codex.git"
  },
  "homepage": "https://github.com/1jmdev/aisdk-codex#readme",
  "bugs": {
    "url": "https://github.com/1jmdev/aisdk-codex/issues"
  },
  "keywords": [
    "ai-sdk",
    "codex",
    "chatgpt",
    "openai",
    "provider"
  ]
}
